blueprint:
  template: development
  shell: /bin/zsh
  image: workspaces/dev:1.10
  sudo: true
  resources:
    cpu: 8
    memory: 8Gi
  network:
    networkPolicy: workspace

  initScripts:
    - init_repo@&: |
        go env -w GOPRIVATE=github.com/fit-students/*
        cd $HOME/gotty
        GOPATH="$HOME/go" go mod tidy
